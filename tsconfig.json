{
    "compilerOptions":{
        "target": "ES2019",//abiente onde vai ser rodado, por ex: internet exploier
        "module": "CommonJS",
        "lib": ["ESNext","DOM"],
        "outDir": "./dist",
        "rootDir": "./src",
        "removeComments": true,
        "noEmitOnError": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "./src"
    ]
}